                <table class="table align-items-center mb-0">
                  <thead>
                    <tr>
                      <th class="text-uppercase text-secondary text-xs font-weight-bolder opacity-10">Game</th>
                      <th class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-10">Topic</th>
                      <th class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-10">Teacher</th>
                      <th class="text-center text-uppercase text-secondary text-xs font-weight-bolder opacity-10">Code</th>
