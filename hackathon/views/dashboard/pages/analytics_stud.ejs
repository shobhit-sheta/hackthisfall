                      <th class="text-secondary opacity-10"></th>
                    </tr>
                  </thead>
                  <tbody>
                    <% for(var i=0; i < quiz.length; i++) { %>

                      <tr>
                        <td>
                          <div class="d-flex px-2 py-1">
                            <div>
                              <img src="/img/user1.jpg" class="avatar avatar-sm me-3" alt="user1">
                            </div>
                            <div class="d-flex flex-column justify-content-center">
                              <h6 class="mb-0 text-sm"><%= quiz[i].game_name %></h6>
                              <!-- <p class="text-xs text-secondary mb-0">john@creative-tim.com</p> -->
                            </div>
                          </div>
                        </td>
                        <td class="align-middle text-center">
                          <span class="text-secondary text-xs font-weight-bold"><%= quiz[i].game_topic %></span>
                        </td>
                        <td class="align-middle text-center">
                          <span class="text-secondary text-xs font-weight-bold"><%= quiz[i].teacher_name %></span>
                        </td>
                        <td class="align-middle text-center">
                          <span class="text-secondary text-xs font-weight-bold"><%= quiz[i].game_id %></span>
                        </td>
                        <td class="align-middle text-center">
                          <span class="text-secondary text-xs font-weight-bold"><%= quiz[i].score %></span>
                        </td>
                        
                      </tr>

                  <% } %>
                    
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
